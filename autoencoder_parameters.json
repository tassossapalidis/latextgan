{   
    "data_parameters": {
        "max_sentence_length": 30,
        "max_vocab": 20000,
        "num_train_examples": 200000,
        "num_dev_examples": 2000
    },
    "training_parameters": {
        "epochs": 8,
        "batch_size": 64,
        "test_dev_frequency": 1,
        "initializer": "glorot_uniform",
        "dropout": 0.4,
        "recurrent_dropout": 0.2,
        "learning_rate": 0.001,
        "weight_decay": 0.00025,
        "beta_1": 0.9,
        "beta_2": 0.999,
        "optimizer": "tf.keras.optimizers.Adam()",
        "loss_function": "tf.keras.losses.SparseCategoricalCrossentropy(from_logits=True, reduction='none')"
    },
    "architecture_parameters": {
        "embedding_dimension": 128,
        "units": 512,
        "encoder_gru_layers": 1,
        "decoder_gru_layers": 1
    },
    "model_save_parameters": {
        "checkpoint_directory": "/content/drive/MyDrive/CS230project/testing_checkpoints/ae_checkpoints",
        "save_frequency": 1,
        "max_to_keep": 2,
        "restore_model": false
    },
    "test_sentences": [
    ]
}
